{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "DistributedRateLimiter": "Debug"
    }
  },
  "AllowedHosts": "*",
  "RateLimiter": {
    "Capacity": 10,
    "RefillRate": 10,
    "RefillIntervalSeconds": 60,
    "RedisHealthCheckIntervalSeconds": 30,
    "EnableMetrics": true
  },
  "Redis": {
    "ConnectionString": "localhost:6379",
    "ConnectTimeout": 500,
    "SyncTimeout": 500,
    "AbortOnConnectFail": false
  }
}
